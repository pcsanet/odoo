<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_inherit_pcsa" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.pcsa</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="guia_remision"/>
                <field name="motivo_traslado"/>
                <field name="establecimiento_origen_id"/>
                <field name="establecimiento_destino_id"/>
            </xpath>
        </field>
    </record>
</odoo>